{
  "name": "aztec-automator",
  "version": "0.1.0",
  "type": "module",
  "exports": "./src/index.ts",
  "publishConfig": {
    "exports": {
      ".": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    },
    "access": "public"
  },
  "scripts": {
    "dev": "tsc -w",
