    "@aztec/noir-contracts.js": "0.87.8",
    "@aztec/stdlib": "0.87.8",
    "ky": "^1.7.1",
    "ms": "^2.1.3",
    "openpgp": "6.1.1",
    "ox": "^0.8.2",
    "ts-essentials": "^10.1.1",
    "ufo": "^1.6.1",
    "vitest": "^3.2.4",
    "zod": "3.25.30"
  },
  "devDependencies": {
    "@types/ms": "^0.7.34",
    "prettier": "^3.5.3",
    "typescript": "5.7.3",
    "vitest": "^3.2.4"
